{
  "compilerOptions": {
    "lib": ["dom", "es2015", "es2016", "es2017"],
    "target": "es2017",
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "outDir": "dist",
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "types": ["cypress", "@types/testing-library__cypress"]
  }
}
